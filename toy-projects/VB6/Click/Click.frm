Attribute VB_Name = "Form1"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Option Explicit
Private level, sec1, sec2, secValue1, secValue2, secValue3, score, overScore As Integer
Private Declare Sub Sleep Lib "kernel32.dll" (ByVal dwMilliseconds As Long)

Private Sub Command1_Click()
Timer1.Interval = 0
Timer1.Interval = 500
sec1 = 0
sec2 = 0
level = 1
Label1.Caption = ""
Label2.Caption = ""
Label3.Caption = ""
score = 0
overScore = 0
Command2.Visible = True
Command1.Visible = False
End Sub

Private Sub Command2_Click()
Timer1.Enabled = True
If (level <> 2 And level <> 3) Then
    level = 1
End If
Command2.Visible = False
End Sub

Private Sub Form_Load()
Dim i As Integer
For i = 1 To 35
Picture1(i).Picture = Picture1(0).Picture
Next
End Sub

Private Sub Picture1_Click(Index As Integer)
    If (Picture1(Index).Visible = True) Then
        score = score + 1
        overScore = overScore - 1
        Label1 = "Score: " & score
        Picture1(Index).Visible = False
        If (score = 10) Then
            Label3.Caption = "Level 2!"
            level = 2
            Timer1.Enabled = False
            Command2.Visible = True
            Timer1.Interval = 650
        End If
        If (score = 40) Then
            Label3.Caption = "Level 3!"
            If (Picture1(secValue1).Visible = True) Then
            Picture1(secValue1).Visible = False
            overScore = overScore - 1
            End If
            If (Picture1(secValue2).Visible = True) Then
            Picture1(secValue2).Visible = False
            overScore = overScore - 1
            End If
            level = 3
            Timer1.Enabled = False
            Command2.Visible = True
            Timer1.Interval = 750
        End If
        If (score = 70) Then
            Label3.Caption = "You win!!"
            If (Picture1(secValue1).Visible = True) Then
            Picture1(secValue1).Visible = False
            overScore = overScore - 1
            End If
            If (Picture1(secValue2).Visible = True) Then
            Picture1(secValue2).Visible = False
            overScore = overScore - 1
            End If
            If (Picture1(secValue3).Visible = True) Then
            Picture1(secValue3).Visible = False
            overScore = overScore - 1
            End If
            Command1.Visible = True
            Timer1.Enabled = False
        End If
    End If
    
End Sub

Private Sub Timer1_Timer()
    sec1 = sec1 + 1
    If (sec2 = 0) Then
        Randomize
        secValue1 = Fix(36 * Rnd)
        Picture1(secValue1).Visible = True
        overScore = overScore + 1
        If (level > 1) Then
level2:
        Randomize
        secValue2 = Fix(36 * Rnd)
        Picture1(secValue2).Visible = True
            If (secValue2 = secValue1) Then GoTo level2
        overScore = overScore + 1
        End If
        If (level > 2) Then
level3:
        Randomize
        secValue3 = Fix(36 * Rnd)
        Picture1(secValue3).Visible = True
            If (secValue3 = secValue2 Or secValue3 = secValue1) Then GoTo level3
        overScore = overScore + 1
        End If
    End If
    If (sec2 = 2) Then
        Picture1(secValue1).Visible = False
        If (level > 1) Then
        Picture1(secValue2).Visible = False
        End If
        If (level > 2) Then
        Picture1(secValue3).Visible = False
        End If
        Label2 = "Miss: " & overScore
        If (overScore > 9) Then
            Label3.Caption = "You lose!!"
            Command1.Visible = True
            Timer1.Enabled = False
            Exit Sub
        End If
        sec2 = -1
    End If
        sec2 = sec2 + 1
        
End Sub


